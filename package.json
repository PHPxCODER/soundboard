{
    "name": "soundboard",
    "version": "1.0.0",
    "description": "Discord bot that stores and plays MP3 files in voice channels",
    "main": "dist/index.js",
    "scripts": {
        "build": "tsc",
        "start": "node -r tsconfig-paths/register dist/index.js",
        "dev": "ts-node -r tsconfig-paths/register src/index.ts",
        "watch": "tsc -w"
    },
    "keywords": [
        "discord",
        "bot",
        "music",
        "mp3",
        "typescript"
    ],
    "author": "RDP Datacenter",
    "license": "MIT",
    "dependencies": {
        "@discordjs/voice": "^0.18.0",
        "discord.js": "^14.19.3",
        "dotenv": "^16.5.0",
        "ffmpeg-static": "^5.2.0",
        "libsodium-wrappers": "^0.7.15",
        "opusscript": "^0.0.8"
    },
    "devDependencies": {
        "@types/node": "^20.19.0",
        "ts-node": "^10.9.2",
        "tsconfig-paths": "^4.2.0",
        "typescript": "^5.8.3"
    },
    "engines": {
        "node": ">=16.0.0"
    }
}
